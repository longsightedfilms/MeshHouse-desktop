{
  "name": "meshhouse",
  "version": "0.1.0",
  "private": false,
  "description": "Open-source 3d models catalogizer",
  "author": {
    "name": "Maxim Makarov",
    "url": "https://github.com/longsightedfilms"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:build:linux": "vue-cli-service electron:build --linux",
    "electron:build:mac": "vue-cli-service electron:build --macos",
    "electron:build:win": "vue-cli-service electron:build --windows",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "electron:serve": "vue-cli-service electron:serve",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "release": "vue-cli-service electron:build --publish always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "electron-store": "^4.0.0",
    "jimp": "^0.8.5",
    "node-notifier": "^5.4.0",
    "recursive-readdir": "^2.2.2",
    "sqlite3": "^4.1.0",
    "transliteration": "^2.1.5",
    "uniqid": "^5.0.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-i18n": "^8.15.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.0.3",
    "vuebar": "0.0.20",
    "vuetify": "^2.1.10",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@mdi/font": "^4.2.95",
    "@types/sqlite3": "^3.1.5",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-typescript": "^5.0.0",
    "babel-eslint": "^10.0.1",
    "electron": "^6.0.7",
    "electron-icon-builder": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.1.0",
    "node-sass": "^4.12.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "vue-cli-plugin-electron-builder": "^1.3.6",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-cli-plugin-typescript": "0.0.1",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "homepage": "https://github.com/longsightedfilms/meshhouse",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/longsightedfilms/meshhouse.git"
  }
}
